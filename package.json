{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "description": "Template",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx .",
    "deploy": "npm run build && npm run copy && gh-pages -d out",
    "manual-deploy": "git checkout gh-pages && git merge master && git add out/* -f && git commit -a -m 'build files' && git push origin gh-pages && git checkout master",
    "copy": "rm -rf out && mkdir -p out && cp index.html out/ && cp -rf images out/images && cp -rf dist out/dist",
    "start": "NODE_ENV=development node server.js"
  },
  "author": "Nikos Katsikanis",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.16.0",
    "normalize.css": "3.0.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "spectacle": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-core": "^6.4.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint": "^1.8.0",
    "eslint-config-defaults": "^7.1.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.11.0",
    "html-loader": "^0.4.0",
    "is-buffer": "^1.1.1",
    "markdown-loader": "^0.1.7",
    "node-libs-browser": "^0.5.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
